{
 "content_scripts": [
    {
      "matches": [ "http://mail.google.com/*", "https://mail.google.com/*"],
      "js": ["jquery.js","gchat.js"]
    }
  ],
 "browser_action": {
 "default_icon": "gift-128.png",
 "default_title": "GChat GIF(t)s"
 },
 "name": "GChat GIF(t)s",
 "description": "Allowing image urls to become embed in gchat",
 "homepage_url": "https://github.com/justintieu/GChat-GIFts/",
 "icons": {
     "128": "gift-128.png" },
 "permissions": [
     "http://*.com/*",
     "https://*.com/*"
 ],
 "version": "0.2.0.1",
 "manifest_version": 2
}
